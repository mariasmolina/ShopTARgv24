@model Guid

@{
    ViewData["Title"] = "Not Found";
}

<div class="text-center mt-5">
    <h1 class="text-danger">404 - Not Found</h1>
    <p>@Model</p>
    <p>The item you are looking for does not exist or may have been removed.</p>
    <a class="btn btn-primary mt-3" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
</div>